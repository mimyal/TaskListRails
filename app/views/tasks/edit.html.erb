<%= form_for @task, match: :patch do |f| %>
  <%= f.label :title %>
  <%= f.text_field :title %>

  <%= f.label :description %>
  <%= f.text_area :description %>

  <%= f.label :owner %>
  <%= f.text_field :owner %>

  <%= f.label :completion_date %>
  <%= f.date_field :completion_date %>

  <%= f.submit %>
<% end %>
<p>
  <!-- <%= link_to "Back to all tasks", "/tasks", class: "" %> -->
  <%= link_to "Get back to tasks view", tasks_path %>
</p>
<p>
</p>
